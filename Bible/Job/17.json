{
  "book_name": "Job",
  "chapter": 17,
  "verses": [
    {
      "book_name": "Job",
      "chapter": 17,
      "verse": 1,
      "text": "רוחי חבלה ימי נזעכו קברים לי",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 17,
      "verse": 2,
      "text": "אםלא התלים עמדי ובהמרותם תלן עיני",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 17,
      "verse": 3,
      "text": "שימהנא ערבני עמך מי הוא לידי יתקע",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 17,
      "verse": 4,
      "text": "כילבם צפנת משכל עלכן לא תרמם",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 17,
      "verse": 5,
      "text": "לחלק יגיד רעים ועיני בניו תכלנה",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 17,
      "verse": 6,
      "text": "והצגני למשל עמים ותפת לפנים אהיה",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 17,
      "verse": 7,
      "text": "ותכה מכעש עיני ויצרי כצל כלם",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 17,
      "verse": 8,
      "text": "ישמו ישרים עלזאת ונקי עלחנף יתערר",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 17,
      "verse": 9,
      "text": "ויאחז צדיק דרכו וטהרידים יסיף אמץ",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 17,
      "verse": 10,
      "text": "ואולם כלם תשבו ובאו נא ולאאמצא בכם חכם",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 17,
      "verse": 11,
      "text": "ימי עברו זמתי נתקו מורשי לבבי",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 17,
      "verse": 12,
      "text": "לילה ליום ישימו אור קרוב מפניחשך",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 17,
      "verse": 13,
      "text": "אםאקוה שאול ביתי בחשך רפדתי יצועי",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 17,
      "verse": 14,
      "text": "לשחת קראתי אבי אתה אמי ואחתי לרמה",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 17,
      "verse": 15,
      "text": "ואיה אפו תקותי ותקותי מי ישורנה",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 17,
      "verse": 16,
      "text": "בדי שאל תרדנה אםיחד עלעפר נחת ס",
      "header": "",
      "footer": ""
    }
  ]
}