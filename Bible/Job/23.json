{
  "book_name": "Job",
  "chapter": 23,
  "verses": [
    {
      "book_name": "Job",
      "chapter": 23,
      "verse": 1,
      "text": "ויען איוב ויאמר",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 23,
      "verse": 2,
      "text": "גםהיום מרי שחי ידי כבדה עלאנחתי",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 23,
      "verse": 3,
      "text": "מייתן ידעתי ואמצאהו אבוא עדתכונתו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 23,
      "verse": 4,
      "text": "אערכה לפניו משפט ופי אמלא תוכחות",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 23,
      "verse": 5,
      "text": "אדעה מלים יענני ואבינה מהיאמר לי",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 23,
      "verse": 6,
      "text": "הברבכח יריב עמדי לא אךהוא ישם בי",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 23,
      "verse": 7,
      "text": "שם ישר נוכח עמו ואפלטה לנצח משפטי",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 23,
      "verse": 8,
      "text": "הן קדם אהלך ואיננו ואחור ולאאבין לו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 23,
      "verse": 9,
      "text": "שמאול בעשתו ולאאחז יעטף ימין ולא אראה",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 23,
      "verse": 10,
      "text": "כיידע דרך עמדי בחנני כזהב אצא",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 23,
      "verse": 11,
      "text": "באשרו אחזה רגלי דרכו שמרתי ולאאט",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 23,
      "verse": 12,
      "text": "מצות שפתיו ולא אמיש מחקי צפנתי אמריפיו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 23,
      "verse": 13,
      "text": "והוא באחד ומי ישיבנו ונפשו אותה ויעש",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 23,
      "verse": 14,
      "text": "כי ישלים חקי וכהנה רבות עמו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 23,
      "verse": 15,
      "text": "עלכן מפניו אבהל אתבונן ואפחד ממנו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 23,
      "verse": 16,
      "text": "ואל הרך לבי ושדי הבהילני",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 23,
      "verse": 17,
      "text": "כילא נצמתי מפניחשך ומפני כסהאפל",
      "header": "",
      "footer": ""
    }
  ]
}