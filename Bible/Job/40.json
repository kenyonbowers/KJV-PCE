{
  "book_name": "Job",
  "chapter": 40,
  "verses": [
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 1,
      "text": "ויען יהוה אתאיוב ויאמר",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 2,
      "text": "הרב עםשדי יסור מוכיח אלוה יעננה פ",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 3,
      "text": "ויען איוב אתיהוה ויאמר",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 4,
      "text": "הן קלתי מה אשיבך ידי שמתי למופי",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 5,
      "text": "אחת דברתי ולא אענה ושתים ולא אוסיף פ",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 6,
      "text": "ויעןיהוה אתאיוב מנ סערה ויאמר",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 7,
      "text": "אזרנא כגבר חלציך אשאלך והודיעני",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 8,
      "text": "האף תפר משפטי תרשיעני למען תצדק",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 9,
      "text": "ואםזרוע כאל לך ובקול כמהו תרעם",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 10,
      "text": "עדה נא גאון וגבה והוד והדר תלבש",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 11,
      "text": "הפץ עברות אפך וראה כלגאה והשפילהו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 12,
      "text": "ראה כלגאה הכניעהו והדך רשעים תחתם",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 13,
      "text": "טמנם בעפר יחד פניהם חבש בטמון",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 14,
      "text": "וגםאני אודך כיתושע לך ימינך",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 15,
      "text": "הנהנא בהמות אשרעשיתי עמך חציר כבקר יאכל",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 16,
      "text": "הנהנא כחו במתניו ואנו בשרירי בטנו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 17,
      "text": "יחפץ זנבו כמוארז גידי פחדו ישרגו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 18,
      "text": "עצמיו אפיקי נחושה גרמיו כמטיל ברזל",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 19,
      "text": "הוא ראשית דרכיאל העשו יגש חרבו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 20,
      "text": "כיבול הרים ישאולו וכלחית השדה ישחקושם",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 21,
      "text": "תחתצאלים ישכב בסתר קנה ובצה",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 22,
      "text": "יסכהו צאלים צללו יסבוהו ערבינחל",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 23,
      "text": "הן יעשק נהר לא יחפוז יבטח כייגיח ירדן אלפיהו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 40,
      "verse": 24,
      "text": "בעיניו יקחנו במוקשים ינקבאף",
      "header": "",
      "footer": ""
    }
  ]
}