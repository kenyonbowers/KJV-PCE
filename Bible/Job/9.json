{
  "book_name": "Job",
  "chapter": 9,
  "verses": [
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 1,
      "text": "ויען איוב ויאמר",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 2,
      "text": "אמנם ידעתי כיכן ומהיצדק אנוש עםאל",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 3,
      "text": "אםיחפץ לריב עמו לאיעננו אחת מניאלף",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 4,
      "text": "חכם לבב ואמיץ כח מיהקשה אליו וישלם",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 5,
      "text": "המעתיק הרים ולא ידעו אשר הפכם באפו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 6,
      "text": "המרגיז ארץ ממקומה ועמודיה יתפלצון",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 7,
      "text": "האמר לחרס ולא יזרח ובעד כוכבים יחתם",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 8,
      "text": "נטה שמים לבדו ודורך עלבמתי ים",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 9,
      "text": "עשהעש כסיל וכימה וחדרי תמן",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 10,
      "text": "עשה גדלות עדאין חקר ונפלאות עדאין מספר",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 11,
      "text": "הן יעבר עלי ולא אראה ויחלף ולאאבין לו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 12,
      "text": "הן יחתף מי ישיבנו מייאמר אליו מהתעשה",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 13,
      "text": "אלוה לאישיב אפו תחתו שחחו עזרי רהב",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 14,
      "text": "אף כיאנכי אעננו אבחרה דברי עמו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 15,
      "text": "אשר אםצדקתי לא אענה למשפטי אתחנן",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 16,
      "text": "אםקראתי ויענני לאאאמין כייאזין קולי",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 17,
      "text": "אשרבשערה ישופני והרבה פצעי חנם",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 18,
      "text": "לאיתנני השב רוחי כי ישבעני ממררים",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 19,
      "text": "אםלכח אמיץ הנה ואםלמשפט מי יועידני",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 20,
      "text": "אםאצדק פי ירשיעני תםאני ויעקשני",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 21,
      "text": "תםאני לאאדע נפשי אמאס חיי",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 22,
      "text": "אחת היא עלכן אמרתי תם ורשע הוא מכלה",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 23,
      "text": "אםשוט ימית פתאם למסת נקים ילעג",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 24,
      "text": "ארץ נתנה בידרשע פנישפטיה יכסה אםלא אפוא מיהוא",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 25,
      "text": "וימי קלו מנירץ ברחו לאראו טובה",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 26,
      "text": "חלפו עםאניות אבה כנשר יטוש עליאכל",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 27,
      "text": "אםאמרי אשכחה שיחי אעזבה פני ואבליגה",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 28,
      "text": "יגרתי כלעצבתי ידעתי כילא תנקני",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 29,
      "text": "אנכי ארשע למהזה הבל איגע",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 30,
      "text": "אםהתרחצתי במושלג והזכותי בבר כפי",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 31,
      "text": "אז בשחת תטבלני ותעבוני שלמותי",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 32,
      "text": "כילאאיש כמני אעננו נבוא יחדו במשפט",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 33,
      "text": "לא ישבינינו מוכיח ישת ידו עלשנינו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 34,
      "text": "יסר מעלי שבטו ואמתו אלתבעתני",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Job",
      "chapter": 9,
      "verse": 35,
      "text": "אדברה ולא איראנו כי לאכן אנכי עמדי",
      "header": "",
      "footer": ""
    }
  ]
}