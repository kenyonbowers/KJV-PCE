  "verses": [
    {
      "book_name": "Psalms",
      "chapter": 124,
      "verse": 1,
      "text": "שיר המעלות לדוד לולי יהוה שהיה לנו יאמרנא ישראל",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 124,
      "verse": 2,
      "text": "לולי יהוה שהיה לנו בקום עלינו אדם",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 124,
      "verse": 3,
      "text": "אזי חיים בלעונו בחרות אפם בנו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 124,
      "verse": 4,
      "text": "אזי המים שטפונו נחלה עבר עלנפשנו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 124,
      "verse": 5,
      "text": "אזי עבר עלנפשנו המים הזידונים",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 124,
      "verse": 6,
      "text": "ברוך יהוה שלא נתננו טרף לשניהם",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 124,
      "verse": 7,
      "text": "נפשנו כצפור נמלטה מפח יוקשים הפח נשבר ואנחנו נמלטנו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 124,
      "verse": 8,
      "text": "עזרנו בשם יהוה עשה שמים וארץ",
      "header": "",
      "footer": ""
    }
  ]
