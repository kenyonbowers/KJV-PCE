  "verses": [
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 1,
      "text": "הודו ליהוה כיטוב כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 2,
      "text": "הודו לאלהי האלהים כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 3,
      "text": "הודו לאדני האדנים כי לעלם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 4,
      "text": "לעשה נפלאות גדלות לבדו כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 5,
      "text": "לעשה השמים בתבונה כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 6,
      "text": "לרקע הארץ עלהמים כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 7,
      "text": "לעשה אורים גדלים כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 8,
      "text": "אתהשמש לממשלת ביום כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 9,
      "text": "אתהירח וכוכבים לממשלות בלילה כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 10,
      "text": "למכה מצרים בבכוריהם כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 11,
      "text": "ויוצא ישראל מתוכם כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 12,
      "text": "ביד חזקה ובזרוע נטויה כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 13,
      "text": "לגזר יםסוף לגזרים כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 14,
      "text": "והעביר ישראל בתוכו כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 15,
      "text": "ונער פרעה וחילו ביםסוף כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 16,
      "text": "למוליך עמו במדבר כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 17,
      "text": "למכה מלכים גדלים כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 18,
      "text": "ויהרג מלכים אדירים כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 19,
      "text": "לסיחון מלך האמרי כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 20,
      "text": "ולעוג מלך הבשן כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 21,
      "text": "ונתן ארצם לנחלה כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 22,
      "text": "נחלה לישראל עבדו כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 23,
      "text": "שבשפלנו זכר לנו כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 24,
      "text": "ויפרקנו מצרינו כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 25,
      "text": "נתן לחם לכלבשר כי לעולם חסדו",
      "header": "",
      "footer": ""
    },
    {
      "book_name": "Psalms",
      "chapter": 136,
      "verse": 26,
      "text": "הודו לאל השמים כי לעולם חסדו",
      "header": "",
      "footer": ""
    }
